{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.2|^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "academe/omnipay-authorizenetapi": " ~3.0",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "anhskohbo/no-captcha": "^3.1",
        "barryvdh/laravel-dompdf": "^2.2",
        "baselrabia/myfatoorah-with-laravel": "^1.0",
        "cartalyst/stripe-laravel": "^v16.0.0",
        "deeplcom/deepl-php": "^1.16",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "instamojo/instamojo-php": "^0.4.0",
        "iyzico/iyzipay-php": "^2.0",
        "james-heinrich/getid3": "^1.9",
        "jeroendesloovere/vcard": "^1.7",
        "kreativdev/saas-installer": "^1.0",
        "laravel-lang/lang": "^15.11.6",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.0",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.2",
        "midtrans/midtrans-php": "^2.5",
        "mollie/laravel-mollie": "^3.0.0",
        "mpdf/mpdf": "^8.2",
        "php-http/guzzle7-adapter": "^1.0",
        "phpmailer/phpmailer": "^6.1",
        "pusher/pusher-php-server": "^5.0",
        "razorpay/razorpay": "2.*",
        "simplesoftwareio/simple-qrcode": "^4.2.0",
        "spatie/laravel-sitemap": "^7.2.0",
        "srmklive/paypal": "^3.0",
        "stichoza/google-translate-php": "^5.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3 || ^10.0",
        "nunomaduro/collision": "^8.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {    
    "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Http/Helpers/Helper.php",
            "app/Helpers/AutoTranslateHelper.php"       
 ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
